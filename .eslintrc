{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "es6": true,
  },
  "settings": {
    "import/parser": "babel-eslint",
    "import/cache": {
      "lifetime": 5,
    },
  },
  "rules": {
    "import/no-extraneous-dependencies": ["error", {"devDependencies": false }],
  },
  "globals": {
    "PRODUCTION": true,
    "DEBUG": true,
    "API": true,
  }
}
